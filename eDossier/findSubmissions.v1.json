{
  "openapi": "3.0.0",
  "info": {
    "title": "findSubmissions",
    "version": "1.04"
  },
  "servers": [
    {
      "url": "http://{host}:9251"
    }
  ],
  "paths": {
    "/command": {
      "post": {
        "summary": "Find Submissions",
        "operationId": "post-command",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "../models/response_command.v1.json"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "result": {
                          "type": "object",
                          "properties": {
                            "submissions": {
                              "type": "object",
                              "required": [
                                "details"
                              ],
                              "properties": {
                                "details": {
                                  "type": "array",
                                  "items": {
                                    "type": "object",
                                    "properties": {
                                      "id": {
                                        "type": "string",
                                        "format": "uuid"
                                      },
                                      "date": {
                                        "type": "string",
                                        "format": "date-time"
                                      },
                                      "status": {
                                        "type": "string",
                                        "enum": [
                                          "pending",
                                          "valid"
                                        ]
                                      },
                                      "candidates": {
                                        "type": "array",
                                        "items": {
                                          "$ref": "../models/organizationSupplierFull.v1.json"
                                        }
                                      },
                                      "requirementResponses": {
                                        "type": "array",
                                        "items": {
                                          "$ref": "../models/requirementResponseShortRelatedCandidate.v1.json"
                                        }
                                      },
                                      "documents": {
                                        "type": "array",
                                        "items": {
                                          "type": "object",
                                          "properties": {
                                            "id": {
                                              "type": "string"
                                            },
                                            "title": {
                                              "type": "string"
                                            },
                                            "description": {
                                              "type": "string"
                                            },
                                            "documentType": {
                                              "type": "string",
                                              "enum": [
                                                "illustration",
                                                "submissionDocuments",
                                                "x_qualificationDocuments",
                                                "x_eligibilityDocuments",
                                                "x_technicalDocuments"
                                              ]
                                            }
                                          },
                                          "required": [
                                            "id",
                                            "title",
                                            "documentType"
                                          ]
                                        }
                                      }
                                    },
                                    "required": [
                                      "id",
                                      "date",
                                      "status",
                                      "candidates"
                                    ]
                                  }
                                }
                              }
                            }
                          },
                          "required": [
                            "submissions"
                          ]
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "allOf": [
                  {
                    "$ref": "../models/request_command.v1.json"
                  },
                  {
                    "type": "object",
                    "properties": {
                      "action": {
                        "type": "string",
                        "enum": [
                          "findSubmissions"
                        ]
                      }
                    }
                  },
                  {
                    "type": "object",
                    "properties": {
                      "params": {
                        "type": "object",
                        "required": [
                          "cpid",
                          "ocid",
                          "pmd",
                          "country",
                          "operationType"
                        ],
                        "properties": {
                          "cpid": {
                            "$ref": "../models/cpid.v1.json"
                          },
                          "ocid": {
                            "$ref": "../models/ocid.v1.json"
                          },
                          "pmd": {
                            "type": "string",
                            "enum": [
                              "GPA",
                              "TEST_GPA",
                              "CF",
                              "TEST_CF",
                              "OF",
                              "TEST_OF",
                              "RT",
                              "TEST_RT"
                            ]
                          },
                          "country": {
                            "type": "string",
                            "example": "MD"
                          },
                          "operationType": {
                            "type": "string",
                            "enum": [
                              "startSecondStage",
                              "qualification",
                              "submissionPeriodEnd",
                              "createPcr"
                            ]
                          }
                        }
                      }
                    },
                    "required": [
                      "params"
                    ]
                  }
                ]
              }
            }
          }
        }
      }
    }
  }
}