{
  "openapi": "3.0.0",
  "info": {
    "title": "templates",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://{host}:9210"
    }
  ],
  "paths": {
    "/templates/{kind}/{pmc}/{lang}/{date}": {
      "post": {
        "summary": "Templates",
        "operationId": "post-templates",
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string"
                  }
                },
                "required": [
                  "file"
                ]
              }
            }
          }
        }
      },
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "kind",
          "in": "path",
          "required": true,
          "description": "тип сущности для которой загружается шаблон (Award-Contract)"
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "pmc",
          "in": "path",
          "required": true,
          "description": "procurement method category"
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "lang",
          "in": "path",
          "required": true,
          "description": "language"
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "date",
          "in": "path",
          "required": true,
          "description": "дата, с которой будет действовать шаблон"
        }
      ]
    }
  },
  "components": {
    "schemas": {}
  }
}