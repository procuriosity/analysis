{
  "openapi": "3.0.0",
  "info": {
    "title": "uploadTemplate",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://{host}:9210"
    }
  ],
  "paths": {
    "/upload-template": {
      "post": {
        "summary": "Upload Template",
        "operationId": "post-upload",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "country",
            "required": true,
            "description": "страна"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "pmd",
            "required": true,
            "description": "тип процедуры"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "documentInitiator",
            "required": true,
            "description": "процесс вызывающий загрузку шаблона"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "lang",
            "description": "язык",
            "required": true
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
