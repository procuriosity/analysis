          "in": "path",
          "required": true,
          "description": "tender process cpid"
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "ocid",
          "in": "path",
          "required": true,
          "description": "contract process ocid"
        }
      ]
    }
  },
  "components": {
    "schemas": {}
  }
