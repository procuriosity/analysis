                              "type": "object",
                              "properties": {
                                "id": {
                                  "type": "string"
                                },
                                "internalId": {
                                  "type": "string"
                                },
                                "classification": {
                                  "type": "object",
                                  "required": [
                                    "id",
                                    "scheme"
                                  ],
                                  "properties": {
                                    "id": {
                                      "type": "string"
                                    },
                                    "scheme": {
                                      "type": "string",
                                      "enum": [
                                        "CPV"
                                      ]
                                    }
                                  }
                                },
                                "additionalClassifications": {
                                  "type": "array",
                                  "items": {
                                    "type": "object",
                                    "properties": {
                                      "id": {
                                        "type": "string"
                                      },
                                      "scheme": {
                                        "type": "string",
                                        "enum": [
                                          "CPVS",
                                          "CPV"
                                        ]
                                      }
                                    },
                                    "required": [
                                      "id",
                                      "scheme"
                                    ]
                                  }
                                },
                                "quantity": {
                                  "type": "number"
                                },
                                "unit": {
                                  "type": "object",
                                  "required": [
                                    "id"
                                  ],
                                  "properties": {
                                    "id": {
                                      "type": "string"
                                    }
                                  }
                                },
                                "description": {
                                  "type": "string"
                                },
                                "relatedLot": {
                                  "type": "string"
                                },
                                "deliveryAddress": {
                                  "$ref": "../models/deliveryAddress.v1.json"
                                }
