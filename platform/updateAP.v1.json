                            "properties": {
                              "startDate": {
                                "type": "string",
                                "format": "date-time"
                              }
                            }
                          },
                          "value": {
                            "type": "object",
                            "properties": {
                              "currency": {
                                "type": "string"
                              }
                            },
                            "required": [
                              "currency"
                            ]
                          },
                          "lots": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "id": {
                                  "type": "string"
                                },
                                "internalId": {
                                  "type": "string"
                                },
                                "title": {
                                  "type": "string"
                                },
                                "description": {
                                  "type": "string"
                                },
                                "placeOfPerformance": {
                                  "type": "object",
