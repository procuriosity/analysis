                                        "addressDetails"
                                      ],
                                      "properties": {
                                        "streetAddress": {
                                          "type": "string"
                                        },
                                        "postalCode": {
                                          "type": "string"
                                        },
                                        "addressDetails": {
                                          "type": "object",
                                          "required": [
                                            "country",
                                            "region"
                                          ],
                                          "properties": {
                                            "country": {
                                              "type": "object",
                                              "required": [
                                                "id"
                                              ],
                                              "properties": {
                                                "id": {
                                                  "type": "string"
                                                }
                                              }
                                            },
                                            "region": {
                                              "type": "object",
                                              "required": [
                                                "id"
                                              ],
                                              "properties": {
                                                "id": {
                                                  "type": "string"
                                                }
                                              }
                                            },
                                            "locality": {
                                              "type": "object",
                                              "properties": {
                                                "id": {
                                                  "type": "string"
                                                },
                                                "scheme": {
                                                  "type": "string"
                                                },
                                                "description": {
                                                  "type": "string"
                                                }
                                              },
                                              "required": [
                                                "id",
                                                "scheme",
                                                "description"
                                              ]
                                            }
                                          }
                                        }
                                      }
                                    }
                                  },
                                  "required": [
                                    "address"
                                  ]
                                }
                              },
                              "required": [
                                "id",
                                "title",
                                "description"
                              ]
                            }
                          },
