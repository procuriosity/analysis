                              "required": [
                                "id",
                                "classification",
                                "quantity",
                                "unit",
                                "description",
                                "relatedLot"
                              ]
                            }
                          },
                          "documents": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "id": {
                                  "type": "string"
                                },
                                "title": {
                                  "type": "string"
                                },
                                "documentType": {
                                  "type": "string",
